<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/btn_start_plugin_service"
        android:text="启动静态service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_stop_plugin_service"
        android:text="停止静态service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_start_dynamic_plugin_service"
        android:text="启动动态service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_stop_dynamic_plugin_service"
        android:text="停止动态service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_start_dynamic_plugin_service2"
        android:text="启动动态service2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <Button
        android:id="@+id/btn_stop_dynamic_plugin_service2"
        android:text="停止动态service2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</LinearLayout>